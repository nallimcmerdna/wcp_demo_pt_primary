                            <!-- BEGIN: Mega Menu -->
                            <ul id="megaMenu" class="collapse navbar-collapse nav navbar-nav">
                                <c:set scope="session" var="portalpage"
                                       value="${navigationContext.defaultNavigationModel.listModel['startNode=/, includeStartNode=false']}"/>
                                 
                                <c:set scope="session" var="maxNumberTopElements" value="5"/>
                                 
                                <c:set scope="session" var="maxNumberSecondaryElements" value="3"/>
                                 
                                <c:set scope="session" var="topNodeCounter" value="0"/>
                                 
                                <c:forEach items="${portalpage}" var="parentNode" varStatus="vs">
                                    <!-- There are a few special page names that are treaed as exceptions to the megamenu -->
                                    <c:if test="${(parentNode.title != 'Top') and (parentNode.title != 'Services') and ((parentNode.title != 'MyCommunities') or ((parentNode.title == 'MyCommunities') and (not empty parentNode.children))) }">
                                        <c:choose>
                                            
                                            <c:when test="${empty parentNode.children}">
                                            <!-- This parentNode has no children -->
                                                <li class="parentnode">
                                                    <a href="/webcenter${parentNode.goLinkPrettyUrl}">
                                                        ${parentNode.title}
                                                    </a>
                                                </li>
                                            </c:when>   
                                            <c:otherwise>
                                            <!-- This topNode has one or more children -->
                                                <li class="parentnode dropdown">
                                                    <a aria-expanded="false" class="dropdown-toggle" data-toggle="dropdown"
                                                        href="#" role="button">
                                                        ${parentNode.title}
                                                        <span class="submenuIndicator pull-right"><i aria-hidden="true" class="fa fa fa-chevron-down"/></span>
                                                    </a>
                                                    
                                                    <c:choose>
                                                        <c:when test="${parentNode.parameters['noOfColumns'] == '2'}">
                                                            <c:set var="colClass" value="col-sm-6"/>
                                                        </c:when>
                                                        <c:when test="${parentNode.parameters['noOfColumns'] == '3'}">
                                                            <c:set var="colClass" value="col-sm-4"/>
                                                        </c:when>
                                                        <c:when test="${parentNode.parameters['noOfColumns'] == '4'}}">
                                                            <c:set var="colClass" value="col-sm-3"/>
                                                        </c:when>
                                                        <c:otherwise>
                                                            <c:set var="colClass" value="col-sm-3"/>
                                                        </c:otherwise>
                                                    </c:choose>

                                                    <ul class="dropdown-menu" role="menu">
                                                        <!-- optionally show a link to the landing page (parent node) -->
                                                        <c:if test="${parentNode.parameters['landingPageShown'] == 'true'}">
                                                            <div class="child-menu row">
                                                                <div class="col-sm-12" >
                                                                    <a class="parentnode" href="/webcenter${parentNode.goLinkPrettyUrl}">
                                                                        ${parentNode.title} Home
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </c:if>
                                                        <!-- loop through each child -->
                                                        <div class="child-menu row">
                                                         <c:forEach items="#{parentNode.children}" var="childNode" varStatus="childLoop">
                                                            <div class="${colClass}" >
                                                                <c:choose>
                                                                    <c:when test="${(childNode.title != 'Separator')}">
                                                                        <a class="childnode" href="/webcenter${childNode.goLinkPrettyUrl}">
                                                                            ${childNode.title}
                                                                        </a>
                                                                        <c:when test="${not empty childNode.children}">
                                                                            <ul class="grandchildmenu" role="menu">
                                                                                <!-- loop through each child -->
                                                                                <c:forEach items="#{childNode.children}" var="grandchildNode" varStatus="grandchildLoop">
                                                                                    <li class="grandchildnode">
                                                                                        <c:when test="${(grandchildNode.title != 'Separator')}">
                                                                                            <a class="grandchildnode" href="/webcenter${grandchildNode.goLinkPrettyUrl}">
                                                                                            ${grandchildNode.title}
                                                                                            </a>
                                                                                        </c:when>
                                                                                    </li>
                                                                                </c:forEach>
                                                                            </ul>
                                                                        </c:when>
                                                                    </c:when>
                                                                </c:choose>
                                                            </div>
                                                         </c:forEach>
                                                            <c:if test="${parentNode.parameters['featureShown'] == 'true'}">
                                                                <div class="${colClass}" >
                                                                    <a class="megamenu-feature" href="${parentNode.parameters['featureLink']}" target="${parentNode.parameters['featureTarget']}">
                                                                        <img src="${parentNode.parameters['featureImage']}" />
                                                                        <p>${parentNode.parameters['featureText']}</p> 
                                                                    </a>
                                                                </div>
                                                            </c:if>
                                                        </div>
                                                    </ul>
                                                </li>
                                            </c:otherwise>
                                        </c:choose>
                                    </c:if>
                                </c:forEach>
