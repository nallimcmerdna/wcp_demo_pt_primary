                                                value="#{WCAppContext.currentScope.default ? boilerBean.copyrightMessage : boilerBean.copyrightMessageInSpace}"/>
                        </div>
                        <div class="col-sm-4">
                            <h3 class="text-color">
                                <span class="border-color">Search</span>
                            </h3>
                            <div id="footerSearchBox">
                                <div class="input-group">
                                    <input type="text" class="form-control" id="bsSearchBox2"
                                           placeholder="Search for people or content..."
                                           onKeydown="Javascript: if(event.keyCode==13) doSearch()"/>
                                     
                                    <span class="input-group-btn">
                                        <a class="btn btn-default" type="button" onclick="doSearch();"
                                           ref="javascript:void(0)">
                                            <span class="glyphicon glyphicon-search" aria-hidden="true"></span></a></span>
                                </div>
                                <div class="input-group">
                                    <label class="radio-inline">
                                        <input type="radio" name="inlineRadioSearchOptionsFtr" id="inlineRadioFtrPeople"
                                               value="option1"/>
                                        People
                                    </label>
                                     
                                    <label class="radio-inline">
                                        <input type="radio" name="inlineRadioSearchOptionsFtr"
                                               id="inlineRadioFtrContent" value="option2" checked="checked"/>
                                        Content
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row footerBottom">
                        <ul class="list-inline">
                            <li>
                                <span class="border-color">Get Social:</span>
                            </li>
                            <li>
                                <a class="twitter" href="#">
                                    <i class="fa fa-twitter"></i></a>
                            </li>
                            <li>
                                <a class="facebook" href="#">
                                    <i class="fa fa-facebook"></i></a>
                            </li>
                            <li>
                                <a class="pinterest" href="#">
                                    <i class="fa fa-pinterest"></i></a>
                            </li>
                            <li>
                                <a class="linkedin" href="#">
                                    <i class="fa fa-linkedin"></i></a>
                            </li>
                            <li>
                                <a class="plus" href="#">
                                    <i class="fa fa-google-plus"></i></a>
                            </li>
                        </ul>
                    </div>
                </footer>
                 
            </div>
            <script language="javascript" src="/webcenter/uilibs/jquery/1.11.1/jquery.min.js" type="text/javascript"></script>
            <script language="javascript" src="/webcenter/uilibs/bootstrap/3.2.0/js/bootstrap.min.js" type="text/javascript"/>
            <script language="javascript" src="/webcenter/uilibs/bootstrap/themes/mosaic/1.0/js/custom.js" type="text/javascript"/>
            <script language="javascript" src="/webcenter/uilibs/bootstrap/themes/mosaic/1.0/js/jquery.sticky.js" type="text/javascript"/> 
            <af:panelGroupLayout id="pt_post" inlineStyle="display:none;" layout="vertical">
                <wcdc:siteTemplateMetadata type="end" id="pt_stm2"/>
            </af:panelGroupLayout>
        </af:panelGroupLayout>
    </af:pageTemplateDef>
</jsp:root>