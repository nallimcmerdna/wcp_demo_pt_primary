            <script language="javascript" src="http://s3.amazonaws.com/pulse-demo-owc12c/pulseUtils.js"
                    type="text/javascript"></script>
            <script>
                
                $(window).scroll(function() {
                    if ($(document).scrollTop() > 50) {
                        $('#fixedHeader').addClass('shrink');
                        $('#content').addClass('adjTopMargin');
                    } else {
                        $('#fixedHeader').removeClass('shrink');
                        $('#content').removeClass('adjTopMargin');
                    }
                });
                
                // ** Search Event Trigger **
                $('#searchBtn').on('click', function(e) {
                    var searchStr = $("input[type=text][id=searchInputText]").val();
                    if(searchStr.length &gt; 1) {
                        alert("Searching for " + searchStr);
                    }
                });                
                
                /* The following event triggers go and get data for the currently logged in user. 
                 * They do so upon button invocation in ajax-type fashion, rather than getting this 
                 * data upon page load. Currently, each "done" function simply inserts a placeholder 
                 * string into the appropriate DOM element. However, they need to be modified to interate
                 * through a pre-defined JSON payload then convert it into its proper HTML. That HTML is
                 * to be inserted into the DOM element instead of the placeholder text.
                 */
                var wsBase = 'https://s3.amazonaws.com/pulse-demo-owc12c/';
                                
                // ** Notification Event Trigger **
                $('#notificationIndicatorBtn').on('click', function(e) {
                    var getNotificationData = $.ajax({
                        url: wsBase + 'notifications.js',
                        type: "GET",
                        cache: false,
                        dataType: "json"
                    });
                    getNotificationData.fail(function(xhr, txtStatus, errorThrown){
                        $('#notification-content').html('error getting JSON: HTTP status=' + xhr.status + ' - ' + textStatus );
                    });
                    getNotificationData.done(function(data, txtStatus, xhr){
                        $('#notification-content').html(data.description);
                    });
                });
                
                // ** Events Calandar Event Trigger **
                $('#dashboard-events-btn').on('click', function(e) {
                    var getEventsData = $.ajax({
                        url: wsBase + 'events.js',
                        type: "GET",
                        cache: false,
                        dataType: "json"
                    });
                    getEventsData.fail(function(xhr, txtStatus, errorThrown){
                        $('#events-content').html('error getting JSON: HTTP status=' + xhr.status + ' - ' + textStatus );
                    });
                    getEventsData.done(function(data, txtStatus, xhr){
                        $('#events-content').html(data.description);
                    });
                });
                
                // ** Favorites Event Trigger **
                $('#dashboard-favorites-btn').on('click', function(e) {
                    var getFavoritesData = $.ajax({
                        url: wsBase + 'favorites.js',
                        type: "GET",
                        cache: false,
                        dataType: "json"
                    });
                    getFavoritesData.fail(function(xhr, txtStatus, errorThrown){
                        $('#favorites-content').html('error getting JSON: HTTP status=' + xhr.status + ' - ' + textStatus );
                    });
                    getFavoritesData.done(function(data, txtStatus, xhr){
                        $('#favorites-content').html(data.description);
                    });
                });
                
                // ** MyApps Event Trigger **//
                $('#dashboard-apps-btn').on('click', function(e) {
                    var getMyAppsData = $.ajax({
                        url: wsBase + 'apps.js',
                        type: "GET",
                        cache: false,
                        dataType: "json"
                    });
                    getMyAppsData.fail(function(xhr, txtStatus, errorThrown){
                        $('#apps-content').html('error getting JSON: HTTP status=' + xhr.status + ' - ' + textStatus );
                    });
                    getMyAppsData.done(function(data, txtStatus, xhr){
                        $('#apps-content').html(data.description);
                    });
                });

            </script>
